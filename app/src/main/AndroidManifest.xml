<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:dist="http://schemas.android.com/apk/distribution"
    package="com.example.motorcycleapp">

    <dist:module dist:instant="true" />

    <uses-permission android:name="android.permission.BLUETOOTH"/>
    <uses-permission android:name="android.permission.BLUETOOTH_ADMIN"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:manageSpaceActivity=".lockdownscreen"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ActivityFirst">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name=".ActivityMainV2">
        </activity>
        <activity android:name=".ActivityThirdV2">
        </activity>
        <activity android:name=".ActivityThird21">
        </activity>
        <activity android:name=".ActivitySeventh2">
        </activity>
        <activity android:name=".ActivitySeventh21">
        </activity>
        <activity android:name=".ActivitySecond21">
        </activity>
        <activity android:name=".ActivityFourthV2">
        </activity>
        <activity android:name=".ActivityFifthV2">
        </activity>
        <activity android:name=".ActivitySixthV2">
        </activity>
        <activity android:name=".ActivitySixthV21">
        </activity>
        <activity android:name=".lockdownscreen">
        </activity>
        <activity android:name=".resetPass">
        </activity>
        <activity android:name=".resetPin">
        </activity>
        <activity android:name=".successfulSignUp">
        </activity>
        <service android:name=".BluetoothLeService" android:enabled="true"/>
        <activity android:name=".activity_splashScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>

        </activity>

    </application>

</manifest>